<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1" />
    </startup>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="UsuarioWSPortBinding" />
                <binding name="ProductoWSPortBinding" />
            </basicHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://localhost:8080/discoteque/UsuarioWS"
                binding="basicHttpBinding" bindingConfiguration="UsuarioWSPortBinding"
                contract="UsuarioService.UsuarioWS" name="UsuarioWSPort" />
            <endpoint address="http://localhost:8080/discoteque/ProductoWS"
                binding="basicHttpBinding" bindingConfiguration="ProductoWSPortBinding"
                contract="ProductoSevice.ProductoWS" name="ProductoWSPort" />
        </client>
    </system.serviceModel>
</configuration>